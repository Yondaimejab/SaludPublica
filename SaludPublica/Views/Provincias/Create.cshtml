@model SaludPublica.Models.Provincia

@{
    ViewData["Title"] = "Crear Provincia";
}

<h2>Crear</h2>

<h4>Provincia</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nombre" class="control-label"></label>
                <input asp-for="Nombre" class="form-control" />
                <span asp-validation-for="Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PaisID" class="control-label"></label>
                <select asp-for="PaisID" class ="form-control" asp-items="ViewBag.PaisID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <iframe id="paisFrame" src="@Url.Action("Create","Pais")" style="width:100%;height:200px"/>
    </div>
</div>

<div>
    <a asp-action="Index">Volver Atras</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script>
        $(document).ready(function () {
            console.log($('#paisFrame form'));
            alert('klk');
        });
    </script>
}
