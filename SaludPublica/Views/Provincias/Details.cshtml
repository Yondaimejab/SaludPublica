@model SaludPublica.Models.Provincia

@{
    ViewData["Title"] = "Detalles Provincia";
}

<h2>Detalles</h2>

<div>
    <h4>Provincia</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Pais)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pais.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ProvinciaID" class="btn btn-primary">Editar</a> |
    <a asp-action="Index">Volver Atras</a>
</div>
